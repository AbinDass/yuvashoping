<%- include('partials/Header.ejs') %> 
<div style="padding: 5%;">
    <table class="table table-striped table-dark" >
        <thead>
           
          <tr>
            <th scope="col">No</th>
            <th scope="col">sub hedding</th>
            <th scope="col">Main Heading</th>
            <th scope="col">type</th>
            <th scope="col">url</th>
            <th scope="col">description</th>
            <th scope="col"> action</th>
           
          </tr>
        </thead>
        <tbody>
            <% for(let i=0;i<banner.length;i++) {%> 
                <tr>
                       
                    <th scope="row"><%= i  %> </th>
                    <td><%- banner[i].subHeading -%>  </td>
                    <td><%= banner[i].mainHeading %>  </td>
                    <td> <%= banner[i].bannerType %>  </td>
                    <td><%= banner[i].url %>  </td>
                    <td><%= banner[i].description %> </td>
                    <td><a href="/admin/deletebanner/<%= banner[i]._id %>" class="btn btn-danger"> Delete banner</a></td>
                </tr>
                <% } %> 
            </tbody>
        </table>
    </div>
<%- include('partials/Footer.ejs') %> 

